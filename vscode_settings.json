// Place your settings in this file to overwrite the default settings
{
	"editor.autoIndent": "full",
	"editor.formatOnSave": true,
	"editor.minimap.enabled": false,
	"editor.tabSize": 4,
	"editor.wordWrapColumn": 100,
	"editor.insertSpaces": false,
	"editor.wrappingStrategy": "simple",
	"editor.wrappingIndent": "same",
	"explorer.confirmDelete": false,
	"explorer.confirmDragAndDrop": false,
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 120000,
	"git.autofetch": true,
	"git.confirmSync": false,
	"git.enableSmartCommit": true,
	"html.autoClosingTags": true,
	"html.format.enable": true,
	"html.format.wrapAttributes": "auto",
	"html.format.wrapLineLength": 250,
	"javascript.suggest.completeFunctionCalls": true,
	"javascript.updateImportsOnFileMove.enabled": "always",
	"path-intellisense.autoSlashAfterDirectory": true,
	"prettier.arrowParens": "avoid",
	"prettier.printWidth": 120,
	"prettier.singleQuote": true,
	"prettier.tabWidth": 4,
	"prettier.trailingComma": "es5",
	"prettier.useTabs": true,
	"typescript.updateImportsOnFileMove.enabled": "always",
	"window.restoreFullscreen": true,
	"window.zoomLevel": 1,
	"workbench.iconTheme": "material-icon-theme",
	"workbench.startupEditor": "newUntitledFile",
	"platformio-ide.disableToolbar": true,
	"platformio-ide.activateOnlyOnPlatformIOProject": false,
	"platformio-ide.disablePIOHomeStartup": true,
	"C_Cpp.clang_format_style": "{ BasedOnStyle: Google, ColumnLimit: 100, IndentWidth: 4, TabWidth: 4}",
	"C_Cpp.clang_format_fallbackStyle": "Google",
	"C_Cpp.default.intelliSenseMode": "clang-x64",
	"C_Cpp.default.compilerPath": "/usr/bin/clang",
	"C_Cpp.default.macFrameworkPath": [
		"/Applications/Xcode.app/Contents/Developer/Platforms/MacOSX.platform/Developer/SDKs/MacOSX10.14.sdk/System/Library/Frameworks"
	]
}

